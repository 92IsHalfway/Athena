use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function athena_damagetakenpermindeltas_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the athena_damagetakenpermindeltas module.
    case 'help.page.athena_damagetakenpermindeltas':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Custom entity storage for damage taken per min deltas.') . '</p>';
      return $output;

    default:
  }
}
